{
    "name": "bkstar123/laravel-cloudflare-buddy",
    "description": "Laravel based tool for interacting with Cloudflare API",
    "keywords": [
        "laravel",
        "cloudflare",
        "cloudflare api",
        "cloudflare api v4"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Hoang Anh Tuan",
            "email": "hoangtuandhbk@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1.3",
        "guzzlehttp/guzzle": "^6.3",
        "spatie/ssl-certificate": "^1.19"
    },
    "autoload": {
        "psr-4": {
            "Bkstar123\\CFBuddy\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bkstar123\\CFBuddy\\LaravelCFBuddyServiceProvider"
            ]
        }
    }
}
